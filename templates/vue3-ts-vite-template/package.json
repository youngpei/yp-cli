    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.3.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^7.0.2",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.4.1",
